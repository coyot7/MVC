@model IEnumerable<string> 
@Html.ActionLink("Create", "Create", "Product", null, new { @class = "btn btn-primary" })
@Html.ActionLink("Home" ,"Index", "Product")

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "Product",
    action = "List",
    category = link,
    page = 1
}
)
}

